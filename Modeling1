####split data into training and testing set using random selection####
splitdata<-split(sb[1:nrow(sb),], sample(rep(1:5, as.integer(nrow(sb)/5))))
test<-splitdata[[4]]
train<-rbind(splitdata[[1]],splitdata[[5]],splitdata[[3]],splitdata[[2]])
str(train)
str(test)
